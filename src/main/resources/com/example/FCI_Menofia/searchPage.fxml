<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="414.0" prefWidth="841.0" style="-fx-background-color: #259ce6;"
            xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.FCI_Menofia.SearchPage">
    <children>
        <Label layoutX="334.0" layoutY="49.0" text="Search Page" textFill="WHITE">
            <font>
                <Font name="Baskerville Old Face" size="36.0"/>
            </font>
        </Label>
        <Button fx:id="backToMainPageButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false"
                onAction="#backToMainPage" prefHeight="34.0" prefWidth="138.0"
                style="-fx-alignment: CENTER; -fx-background-radius: 50PX; -fx-background-color: #ffffff;"
                text="back to Main page">
            <font>
                <Font size="13.0"/>
            </font>
        </Button>
        <HBox alignment="CENTER" layoutX="97.0" layoutY="100.0" prefHeight="100.0" prefWidth="607.0" spacing="15.0">
            <children>
                <Label text="Enter National id :">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Label>
                <TextField fx:id="searchTextField" prefHeight="38.0" prefWidth="219.0" promptText="Search"
                           style="-fx-alignment: CENTER; -fx-background-radius: 10PX;"/>
                <Button fx:id="searchButton" mnemonicParsing="false"
                        onAction="#searchForStudentInDatabaseUsingNationalId" prefHeight="34.0" prefWidth="85.0"
                        style="-fx-background-color: #ffffff; -fx-background-radius: 50PX;" text="Search">
                    <font>
                        <Font name="System Bold Italic" size="15.0"/>
                    </font>
                </Button>
            </children>
        </HBox>
        <TableView fx:id="tabel" layoutY="200.0" prefHeight="216.0" prefWidth="841.0">
            <columns>
                <TableColumn fx:id="name" prefWidth="75.0" text="name"/>
                <TableColumn fx:id="level" prefWidth="75.0" text="level"/>
                <TableColumn fx:id="department" prefWidth="75.0" text="department"/>
                <TableColumn fx:id="section" prefWidth="75.0" text="section"/>
                <TableColumn fx:id="jpa" prefWidth="75.0" text="jpa"/>
                <TableColumn fx:id="course1" prefWidth="75.0" text="course1"/>
                <TableColumn fx:id="course2" prefWidth="75.0" text="course2"/>
                <TableColumn fx:id="course3" prefWidth="75.0" text="course3"/>
                <TableColumn fx:id="course4" prefWidth="75.0" text="course4"/>
                <TableColumn fx:id="course5" prefWidth="75.0" text="course5"/>
                <TableColumn fx:id="course6" prefWidth="75.0" text="course6"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
